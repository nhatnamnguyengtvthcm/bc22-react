 index = state.gioHang.findIndex(obj => {return obj.maSP == action.data.maSP});\n      if (index!= -1){\n        state.gioHang[index].soLuong +=1;\n      }\n      else{\n        state.gioHang.push(action.data);\n      }\n      state.gioHang = [...state.gioHang]\n      return {...state};\n    default:\n      break;\n  }\n  return state;\n};\n","/Users/namnn/Documents/cybersoft/bc22-react/src/BT_GioHang/BTGioHangRedux.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_GioHang/DanhSachSanPhamRedux.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_GioHang/ModalGioHangRedux.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_GioHang/SanPhamRedux.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/redux/constants/gioHangConstants.js",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/16_Style/DemoJSS.jsx",["208"],[],"import React from 'react'\nimport { TextField } from './TextField';\nimport { ThemeProvider, createGlobalStyle } from \"styled-components\";\nconst DemoJSS = () => {\n  return (\n    <div>\n        <ThemeProvider theme={}>\n        <TextField>Nam</TextField>\n        </ThemeProvider>\n        \n\n    </div>\n  )\n}\n\nexport default DemoJSS","/Users/namnn/Documents/cybersoft/bc22-react/src/16_Style/TextField.jsx",[],[],[],{"ruleId":"209","severity":1,"message":"210","line":16,"column":6,"nodeType":"211","endLine":16,"endColumn":22,"suggestions":"212"},{"ruleId":"213","severity":1,"message":"214","line":4,"column":10,"nodeType":"215","messageId":"216","endLine":4,"endColumn":14},{"ruleId":"209","severity":1,"message":"217","line":42,"column":6,"nodeType":"211","endLine":42,"endColumn":20,"suggestions":"218"},{"ruleId":"219","severity":1,"message":"220","line":51,"column":19,"nodeType":"221","endLine":51,"endColumn":69},{"ruleId":"209","severity":1,"message":"217","line":29,"column":6,"nodeType":"211","endLine":29,"endColumn":8,"suggestions":"222"},{"ruleId":"213","severity":1,"message":"223","line":5,"column":8,"