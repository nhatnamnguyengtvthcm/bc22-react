assName=\"bg-secondary\">\n      <div className=\"container\">\n        <h1 className=\"text-center text-white\">My todos</h1>\n\n        <div className=\"row\">\n          <div className=\"col-sm-6 mx-auto text-white\">\n            <AddTodo />\n\n            <SearchTodo />\n\n            <TodoList />\n\n            <FilterTodo />\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default TodoApp;\n","/Users/namnn/Documents/cybersoft/bc22-react/src/BT_HomePage/Home.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_MovieTicket/MovieTicket.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/redux/reducers/todoReducer.js",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/redux/reducers/colorReducer.js",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/redux/reducers/countReducer.js",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_TodoAppAPI/AddTodo.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_TodoAppAPI/FilterTodo.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_TodoAppAPI/SearchTodo.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_TodoAppAPI/TodoList.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_HomePage/Header.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_HomePage/Content.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_MovieTicket/SeatList.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/redux/actions/todoActions.js",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_HomePage/Navigation.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_HomePage/Footer.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_MovieTicket/TicketForm.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_MovieTicket/TicketConfirm.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/redux/constants/todoConstants.js",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_TodoAppAPI/TodoItem.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/BT_MovieTicket/SeatItem.jsx",[],[],"/Users/namnn/Documents/cybersoft/bc22-react/src/15_Router/MovieList.jsx",["201","202","203"],[],"import axios from \"axios\";\nimport React, { useState } from \"react\";\nimport { useEffect } from \"react\";\nimport { Link, useNavigate, useSearchParams } from \"react-router-dom\";\nconst MovieList = () => {\n  const [movies, setMovies] = useState([]);\n\n \n\n  // hook useNavigate return về 1 func dùng để điều hướng url\n  const navigate = useNavigate();\n  const gotoDetails = (movieId) => {\n    navigate(`/movies/${movieId}`);\n  };\n\n  // hooks useSearchParams return về 1 array gồm 2 phần tử\n  // - searchParams: một đối tượng URLSearchParams\n  // - setSearchParams: dùng để thay đổi giá trị search params trên url\n\n  const [searchParams, setSearchParams] = useSearchParams();\n  const changePage = (page) => {\n    setSearchParams({page});\n  };\n  const fetchMovie = async () => {\n    try {\n      const { data } = await axios.get(\n        \"https://629757b414e756fe3b2dc8f0.mockapi.io/api/movies\",{\n          params:{\n            page:searchParams.get(\"page\") || 1,\n            limit:4\n          }  \n        }\n      );\n      setMovies(data);\n    } catch {\n      console.log(\"Error\");\n    }\n  };\n\n  useEffect(() => {\n    fetchMovie();\n  }, [searchParams]);\n  return (\n    <div>\n      <div className=\"container\">\n        <div className=\"row\">\n          {movies.map((movie) => {\n            return (\n              <div key={movie.id} className=\"col-sm-3\">\n                <div className=\"card\">\n                  <img className=\"card-img-top\" src={movie.image} />\n                  <div className=\"card-body\">\n                    <h4 className=\"card-title\">{movie.name}</h4>\n                    <p className=\"card-text\">{movie.description}</p>\n                    {/* C1: Dùng thẻ link để điều hướng  */}\n                    {/* <Link className=\"btn btn-success\" to={`/movies/${movie.id}`}>Chi tiết</Link> */}\n\n                    {/* C2: Goi hàm÷ */}\n                    <button\n                      className=\"btn btn-success\"\n                      onClick={() => gotoDetails(movie.id)}\n                    >\n                      Chi tiết\n                    </button>\n                  </div>\n                </div>\n              </div>\n            );\n          })}\n        </div>\n       \n      </div>\n      <button className=\"btn btn-success\" onClick={() => changePage(1)}>\n          1\n        </button>\n        <button className=\"btn btn-success\" onClick={() => changePage(2)}>\n          2\n        </button>\n        <button className=\"btn btn-success\" onClick={() => changePage(3)}>\n          3\n        </button>\n    </div>\n  );\n};\n\nexport default MovieList;\n","/Users/namnn/Documents/cybersoft/bc22-react/src/15_Router/MovieDetail.jsx",["204"],[],"import axios from \"axios\";\nimport React, { useState } from \"react\";\nimport { useEffect } from \"react\";\nimport { useParams, useNavigate} from \"react-router-dom\";\nconst MovieDetail = () => {\n  // hook useParams return về 1 obj có các properties là các params trên url\n  // path: \"/movies/:movieId\" => {movieId: \"1\"}\n  const { movieId } = useParams();\n  // console.log(\"movieId: \");\n  console.log(movieId);\n  const [movie, setMovie] = useState(null);\n\n  const navigate = useNavigate();\n  const fetchMovie = async () => {\n    try {\n      const { data } = await axios.get(\n        `https://629757b414e756fe3b2dc8f0.mockapi.io/api/movies/${movieId}`\n      );\n      setMovie(data);\n    } catch(error) {\n    //   console.log(\"Error\");\n    // Call API fail => movieId không hợp lệ => navigate về trang notFound\n        navigate('/404');\n    }\n  };\n\n  useEffect(() => {\n    fetchMovie();\n  }, []);\n  if (!movie) return null;\n  return (\n    <div>\n      <h1>Tên Phim: {movie.name}</h1>\